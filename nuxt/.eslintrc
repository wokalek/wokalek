{
  "root": true,
  "extends": [
    "@nuxtjs/eslint-config-typescript"
  ],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "func-call-spacing": 0,
    "vue/multi-word-component-names": 0,
    "vue/no-multiple-template-root": 0,
    "vue/singleline-html-element-content-newline": 0
  }
}
